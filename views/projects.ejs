        <div class="container">
          <h1 class="text-center section_headings ltyellow_text">Projects</h1>

          <!-- ROW 1 -->
          <div class="row row-projects">
            <!-- SD CHALLENGE Section -->
            <div class="col-md-6 portfolio-item text-center">
              <a class="portfolio-link" href="#portfolioModal1" data-toggle="modal">
                <img class="project_images" src="images/sdchallenge.jpg" alt="SD Challenge">
                <p class="lead">The SD Challenge</p
              </a>
            </div>

            <!-- WHOSE WHO OF GITHUB Section -->
            <div class="col-md-6 portfolio-item text-center">
              <a class="portfolio-link" href="#portfolioModal2" data-toggle="modal">
                <img class="project_images" src="images/github-return.jpg" alt="Whose Who of GitHub">
                <p class="lead">Whose Who of GitHub</p>
              </a>
            </div>
          </div>

          <!-- ROW 2 -->
          <div class="row row-projects">
            <!-- ROVERPIX Section -->
            <div class="col-md-6 portfolio-item text-center">
              <a class="portfolio-link" href="#portfolioModal3" data-toggle="modal">
                <img class="project_images" src="images/roverpix.jpg" alt="RoverPix">
                <p class="lead">RoverPix</p>
              </a>
            </div>

            <!-- SUBHUB Section -->
            <div class="col-md-6 portfolio-item text-center">
              <a class="portfolio-link" href="#portfolioModal4" data-toggle="modal">
                <img class="project_images" src="images/subhub.jpg" alt="SubHub">
                <p class="lead">SubHub</p>
              </a>
            </div>
          </div>
        </div>
